CREATE TABLE `user_daily_order_82` (
  `day_time` date DEFAULT NULL COMMENT '统计日期',
  `name` varchar(100) DEFAULT NULL COMMENT '姓名',
  `nickname` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '用户昵称',
  `phone` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '手机号',
  `unionid` bigint(20) DEFAULT NULL COMMENT '用户id',
  `operate_id` bigint(20) DEFAULT NULL COMMENT '运营中心id',
  `operatename` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '运营中心名称',
  `leader_unionid` bigint(20) DEFAULT NULL COMMENT '运营中心负责人id',
  `leader` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '运营中心负责人',
  `leader_phone` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '运营中心负责人手机号',
  `parentid` bigint(20) DEFAULT NULL COMMENT '上级id',
  `parent_phone` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '上级手机号',
  `buy_count` int(11) DEFAULT '0' COMMENT '采购订单数量',
  `buy_pretty_count` int(11) DEFAULT '0' COMMENT '采购靓号数量',
  `buy_total_price` decimal(17,2) DEFAULT '0.00' COMMENT '采购金额',
  `publish_count` int(11) DEFAULT '0' COMMENT '发布订单数量',
  `publish_pretty_count` int(11) DEFAULT '0' COMMENT '发布靓号数量',
  `publish_total_price` decimal(17,2) DEFAULT '0.00' COMMENT '发布金额',
  `sell_count` int(11) DEFAULT '0' COMMENT '出售订单数量',
  `sell_pretty_count` int(11) DEFAULT '0' COMMENT '出售靓号数量',
  `sell_total_price` decimal(17,2) DEFAULT '0.00' COMMENT '出售金额',
  `truth_price` decimal(17,2) DEFAULT '0.00' COMMENT '出售实收金额',
  `sell_fee` decimal(17,2) DEFAULT '0.00' COMMENT '出售手续费',
  `addtime` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '入库时间',
  `updtime` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='用户每日订单数据统计表';



